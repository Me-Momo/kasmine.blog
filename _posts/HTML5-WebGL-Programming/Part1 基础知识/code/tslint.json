{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-var-keyword": true,
        "no-var-requires": false,
        "one-variable-per-declaration": false,
        "no-reference": false,
        "no-unused-variable": true,
        "no-unused-expression": true,
        "object-literal-sort-keys": false,
        "arrow-parens": false,
        "variable-name": false
    },
    "rulesDirectory": []
}